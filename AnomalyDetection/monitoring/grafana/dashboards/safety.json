{
  "title": "Safety Traceability Overview",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "ASIL Level Changes (rate)",
      "datasource": { "type": "prometheus", "uid": "PROM" },
      "targets": [ { "expr": "sum(rate(asil_level_change_total[5m])) by (new_level)" } ],
      "fieldConfig": { "defaults": { "unit": "changes/min" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "ASIL Re-Review Triggers",
      "datasource": { "type": "prometheus", "uid": "PROM" },
      "targets": [ { "expr": "increase(asil_re_review_trigger_total[30m])" } ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Re-Review Trigger Rate % (5m)",
      "datasource": { "type": "prometheus", "uid": "PROM" },
      "targets": [ { "expr": "(sum(rate(asil_re_review_trigger_total[5m])) / clamp_min(sum(rate(asil_level_change_total[5m])), 1e-9)) * 100" } ],
      "fieldConfig": { "defaults": { "unit": "percent" } },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 8 }
    },
    {
      "type": "timeseries",
      "title": "ASIL Change Matrix (old->new)",
      "datasource": { "type": "prometheus", "uid": "PROM" },
      "targets": [ { "expr": "sum(rate(asil_level_change_total[10m])) by (old_level,new_level)" } ],
      "gridPos": { "h": 10, "w": 18, "x": 6, "y": 8 }
    }
  ]
}
